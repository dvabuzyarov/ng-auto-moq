{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-auto-moq --configuration production && cpx README.md ./dist/ng-auto-moq/",
    "tests:clean": "npx rimraf ./out-tsc",
    "tests:build": "npx ts-node --esm --project tsconfig.json ./spec:build.mts",
    "tests:coverage": "c8 npm test",
    "test": "npm run tests:clean && npm run tests:build && node --loader=esm-module-alias/loader ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry"
  },
  "private": true,
  "aliases": {
    "ng-auto-moq": "out-tsc/spec/src/public_api.js"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1401.3",
    "@angular-devkit/build-angular": "14.1.3",
    "@angular-devkit/core": "14.1.3",
    "@angular-eslint/builder": "14.0.0",
    "@angular-eslint/eslint-plugin": "14.0.0",
    "@angular-eslint/eslint-plugin-template": "14.0.0",
    "@angular-eslint/template-parser": "14.0.0",
    "@angular/cli": "14.1.3",
    "@angular/common": "14.1.3",
    "@angular/compiler": "14.1.3",
    "@angular/compiler-cli": "14.1.3",
    "@angular/core": "14.1.3",
    "@angular/language-service": "14.1.3",
    "@commitlint/cli": "17.0.3",
    "@commitlint/config-conventional": "17.0.3",
    "@testdozer/ng-injector-types": "1.0.2",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.10",
    "@types/node": "16.11.41",
    "@typescript-eslint/eslint-plugin": "5.29.0",
    "@typescript-eslint/parser": "5.29.0",
    "c8": "7.12.0",
    "core-js": "3.24.1",
    "cpx": "1.5.0",
    "cz-conventional-changelog": "3.3.0",
    "esbuild": "0.15.5",
    "eslint": "8.22.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsdoc": "39.3.6",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "esm-module-alias": "2.0.1",
    "husky": "8.0.1",
    "jasmine": "4.3.0",
    "jasmine-core": "4.3.0",
    "jasmine-spec-reporter": "7.0.0",
    "moq.ts": "9.0.2",
    "ng-packagr": "14.1.0",
    "rxjs": "7.5.6",
    "semantic-release": "19.0.3",
    "ts-node": "10.9.1",
    "tslib": "2.4.0",
    "typescript": "4.7.4",
    "zone.js": "0.11.6"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-auto-moq"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}

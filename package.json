{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-auto-moq --prod && cpx README.md ./dist/ng-auto-moq/",
    "build-tests": "tsc -p ./projects/ng-auto-moq/tsconfig.spec.json ",
    "test": "npm run build-tests && node ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/build-angular": "0.901.2",
    "@angular-devkit/build-ng-packagr": "0.901.2",
    "@angular/cli": "9.1.2",
    "@angular/common": "9.1.2",
    "@angular/compiler": "9.1.2",
    "@angular/compiler-cli": "9.1.2",
    "@angular/core": "9.1.2",
    "@angular/language-service": "9.1.2",
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@types/jasmine": "3.5.10",
    "@types/jasminewd2": "2.0.8",
    "@types/node": "12.12.36",
    "codelyzer": "5.2.2",
    "cpx": "1.5.0",
    "rxjs": "6.5.5",
    "zone.js": "0.10.3",
    "cz-conventional-changelog": "3.1.0",
    "husky": "4.2.5",
    "jasmine": "3.5.0",
    "jasmine-core": "3.5.0",
    "jasmine-spec-reporter": "4.2.1",
    "moq.ts": "5.0.5",
    "ng-packagr": "9.1.1",
    "semantic-release": "17.0.6",
    "ts-node": "8.9.0",
    "tslint": "5.20.1",
    "tslib": "1.11.1",
    "typescript": "3.7.5"
  },
  "release": {
    "pkgRoot": "dist/ng-auto-moq"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  }
}

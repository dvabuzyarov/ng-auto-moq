{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-auto-moq --prod && cpx README.md ./dist/ng-auto-moq/",
    "build-tests": "tsc -p ./projects/ng-auto-moq/tsconfig.spec.json ",
    "test": "npm run build-tests && node ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry",
    "semantic-release": "semantic-release"
  },
  "private": true,
  "devDependencies": {
    "@angular-devkit/architect": "0.1100.5",
    "@angular-devkit/build-angular": "0.1100.5",
    "@angular-devkit/core": "11.0.5",
    "@angular-eslint/builder": "1.0.0",
    "@angular-eslint/eslint-plugin": "1.0.0",
    "@angular-eslint/eslint-plugin-template": "1.0.0",
    "@angular-eslint/template-parser": "0.8.0-beta.7",
    "@angular/cli": "11.0.5",
    "@angular/common": "11.0.5",
    "@angular/compiler": "11.0.5",
    "@angular/compiler-cli": "11.0.5",
    "@angular/core": "11.0.5",
    "@angular/language-service": "11.0.5",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.8",
    "@typescript-eslint/eslint-plugin": "4.11.1",
    "@typescript-eslint/parser": "4.11.1",
    "@types/node": "12.19.11",
    "cpx": "1.5.0",
    "eslint": "7.16.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsdoc": "30.7.9",
    "eslint-plugin-prefer-arrow": "1.2.2",
    "rxjs": "6.6.3",
    "zone.js": "0.11.3",
    "cz-conventional-changelog": "3.3.0",
    "husky": "4.3.6",
    "jasmine": "3.6.3",
    "jasmine-core": "3.6.0",
    "jasmine-spec-reporter": "6.0.0",
    "moq.ts": "7.0.0",
    "ng-packagr": "11.0.3",
    "semantic-release": "17.3.1",
    "ts-node": "9.1.1",
    "typescript": "4.0.5",
    "tslib": "2.0.3"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-auto-moq"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {}
}

{
  "name": "",
  "version": "0.0.0-semantically-released",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build ng-auto-moq --configuration production && cpx README.md ./dist/ng-auto-moq/",
    "tests:build": "tsc -p ./projects/ng-auto-moq/tsconfig.spec.json ",
    "test": "npm run tests:build && node ./out-tsc/**/index.specs.js",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "commit": "git-cz",
    "commit-retry": "git cz --retry"
  },
  "private": true,
  "_moduleAliases": {
    "ng-auto-moq": "out-tsc/spec/src/public_api.js"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1300.1",
    "@angular-devkit/build-angular": "12.2.13",
    "@angular-devkit/core": "12.2.13",
    "@angular-eslint/builder": "12.6.1",
    "@angular-eslint/eslint-plugin": "12.6.1",
    "@angular-eslint/eslint-plugin-template": "12.6.1",
    "@angular-eslint/template-parser": "12.6.1",
    "@angular/cli": "12.2.13",
    "@angular/common": "12.2.13",
    "@angular/compiler": "12.2.13",
    "@angular/compiler-cli": "12.2.13",
    "@angular/core": "12.2.13",
    "@angular/language-service": "12.2.13",
    "@commitlint/cli": "14.1.0",
    "@commitlint/config-conventional": "14.1.0",
    "@types/jasmine": "3.6.2",
    "@types/jasminewd2": "2.0.10",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "@types/node": "16.11.6",
    "cpx": "1.5.0",
    "eslint": "7.32.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jsdoc": "37.0.3",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "rxjs": "7.4.0",
    "zone.js": "0.11.4",
    "cz-conventional-changelog": "3.3.0",
    "husky": "7.0.4",
    "jasmine": "3.10.0",
    "jasmine-core": "3.10.1",
    "jasmine-spec-reporter": "7.0.0",
    "moq.ts": "7.3.4",
    "ng-packagr": "12.2.5",
    "semantic-release": "17.4.7",
    "ts-node": "10.4.0",
    "typescript": "4.3.5",
    "tslib": "2.3.1",
    "core-js": "3.19.1",
    "@testdozer/ng-injector-types": "1.0.2",
    "module-alias": "2.2.2"
  },
  "release": {
    "branches": [
      {
        "name": "master"
      },
      {
        "name": "dev",
        "channel": "next"
      }
    ],
    "pkgRoot": "dist/ng-auto-moq"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run test",
      "commit-msg": "commitlint --edit"
    }
  },
  "dependencies": {}
